{
  "manifest_version": 2,
  "name": "uitcheckgemist.nl filler",
  "description": "Fill out those numbers",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.uitcheckgemist.nl/*"],
      "js": [
        "filler.js"
      ]
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "storage"
  ]
}